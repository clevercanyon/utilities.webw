{
	"private": false,
	"version": "1.0.9",
	"license": "GPL-3.0-or-later",
	"name": "@clevercanyon/utilities.webw",
	"description": "Another great project by Clever Canyon.",
	"repository": "https://github.com/clevercanyon/utilities.webw",
	"homepage": "https://github.com/clevercanyon/utilities.webw#readme",
	"bugs": "https://github.com/clevercanyon/utilities.webw/issues",
	"funding": "https://github.com/sponsors/clevercanyon",
	"keywords": ["clevercanyon"],
	"author": {
		"name": "Clever Canyon",
		"url": "https://clevercanyon.com"
	},
	"contributors": [],
	"type": "module",
	"files": ["/dist"],
	"bin": {},
	"imports": {},
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.umd.cjs",
			"types": "./dist/types/index.d.ts"
		}
	},
	"sideEffects": ["./src/*.{html,scss,css,tsx,ts,jsx,mjs,js}"],
	"module": "./dist/index.js",
	"main": "./dist/index.umd.cjs",
	"browser": "./dist/index.umd.cjs",
	"unpkg": "./dist/index.umd.cjs",
	"types": "./dist/types/index.d.ts",
	"typesVersions": {
		">=3.1": {
			"./*": ["./dist/types/*"]
		}
	},
	"dependencies": {},
	"peerDependencies": {},
	"peerDependenciesMeta": {},
	"optionalDependencies": {},
	"bundleDependencies": [],
	"devDependencies": {
		"@clevercanyon/skeleton-dev-deps": "*"
	},
	"overrides": {},
	"cpu": ["x64", "arm64"],
	"os": ["darwin", "linux"],
	"engines": {
		"node": "^19.2.0",
		"npm": "^8.19.3"
	},
	"scripts": {
		"preview": "npx vite preview",
		"dev": "npx vite dev --mode=dev",
		"envs:push": "./dev/.files/bin/envs.js push",
		"envs:pull": "./dev/.files/bin/envs.js pull",
		"envs:setup": "./dev/.files/bin/envs.js setup",
		"envs:setup:new": "./dev/.files/bin/envs.js setup --new",
		"build": "npx vite build --mode=prod && npx tsc",
		"build:dev": "npx vite build --mode=dev && npx tsc",
		"build:ci": "npx vite build --mode=ci && npx tsc",
		"build:stage": "npx vite build --mode=stage && npx tsc",
		"build:prod": "npx vite build --mode=prod && npx tsc",
		"wrangler": "CLOUDFLARE_API_TOKEN=${C10N_CLOUDFLARE_TOKEN} npx wrangler",
		"update:dotfiles": "./dev/.files/bin/update.js dotfiles",
		"update:project": "npm run update:dotfiles && ./dev/.files/bin/update.js project",
		"update:project:repos": "npm run update:dotfiles && ./dev/.files/bin/update.js project --repos",
		"update:project:repos:pkgs": "npm run update:dotfiles && ./dev/.files/bin/update.js project --repos --pkgs",
		"update:projects::": "./dev/.files/bin/update.js projects",
		"update:projects::dotfiles": "./dev/.files/bin/update.js projects --run update:dotfiles",
		"update:projects::project": "./dev/.files/bin/update.js projects --run update:project",
		"update:projects::project:repos": "./dev/.files/bin/update.js projects --run update:project:repos",
		"update:projects::project:repos:pkgs": "./dev/.files/bin/update.js projects --run update:project:repos:pkgs"
	},
	"workspaces": [],
	"config": {
		"c10n": {
			"&": {
				"dotfiles": {
					"lock": []
				},
				"build": {
					"appType": "cma",
					"targetEnv": "webw"
				}
			}
		}
	}
}
