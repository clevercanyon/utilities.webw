{
	"private": true,
	"version": "1.0.1",
	"license": "GPL-3.0-or-later",
	"name": "@clevercanyon/utilities.webw",
	"description": "Another great project by Clever Canyon.",
	"repository": "https://github.com/clevercanyon/utilities.webw",
	"homepage": "https://github.com/clevercanyon/utilities.webw#readme",
	"bugs": "https://github.com/clevercanyon/utilities.webw/issues",
	"funding": "https://github.com/sponsors/clevercanyon",
	"keywords": ["clevercanyon"],
	"author": {
		"name": "Clever Canyon",
		"url": "https://clevercanyon.com"
	},
	"contributors": [],
	"type": "module",
	"files": ["/dist"],
	"bin": {},
	"imports": {},
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.umd.cjs"
		}
	},
	"sideEffects": ["./src/*.scss", "./src/index.*"],
	"module": "./dist/index.js",
	"main": "./dist/index.umd.cjs",
	"unpkg": "./dist/index.umd.cjs",
	"browser": "./dist/index.umd.cjs",
	"types": "./dist/types/index.d.ts",
	"dependencies": {},
	"bundleDependencies": [],
	"peerDependenciesMeta": {},
	"devDependencies": {
		"@clevercanyon/skeleton-dev-deps": "*"
	},
	"overrides": {},
	"cpu": ["x64", "arm64"],
	"os": ["darwin", "linux"],
	"engines": {
		"node": "^19.2.0",
		"npm": "^8.19.3"
	},
	"scripts": {
		"preview": "npx vite preview",
		"dev": "npx vite dev --mode=dev",
		"build:dev": "npx tsc && npx vite build --mode=dev",
		"build:stage": "npx tsc && npx vite build --mode=stage",
		"build:prod": "npx tsc && npx vite build --mode=prod",
		"dotfiles:update": "./dev/.files/bin/update.js"
	},
	"workspaces": [],
	"publishConfig": {
		"registry": "https://npm.pkg.github.com"
	},
	"config": {
		"c10n": {
			"&": {
				"dotfiles": {
					"lock": []
				},
				"build": {
					"appType": "cma",
					"targetEnv": "webw"
				}
			}
		}
	}
}
