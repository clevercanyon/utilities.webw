{
    "private": false,
    "publishConfig": {
        "access": "restricted"
    },
    "version": "1.0.13",
    "license": "GPL-3.0-or-later",
    "name": "@clevercanyon/utilities.webw",
    "description": "Another great project by Clever Canyon.",
    "repository": "https://github.com/clevercanyon/utilities.webw",
    "homepage": "https://github.com/clevercanyon/utilities.webw#readme",
    "bugs": "https://github.com/clevercanyon/utilities.webw/issues",
    "funding": "https://github.com/sponsors/clevercanyon",
    "keywords": [
        "clevercanyon"
    ],
    "author": {
        "name": "Clever Canyon",
        "url": "https://clevercanyon.com"
    },
    "contributors": [],
    "type": "module",
    "files": [
        "/dist"
    ],
    "bin": {},
    "imports": {},
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.umd.cjs",
            "types": "./dist/types/index.d.ts"
        }
    },
    "sideEffects": [
        "./src/*.{html,scss,css,tsx,ts,jsx,mjs,js}"
    ],
    "module": "./dist/index.js",
    "main": "./dist/index.umd.cjs",
    "browser": "./dist/index.umd.cjs",
    "unpkg": "./dist/index.umd.cjs",
    "types": "./dist/types/index.d.ts",
    "typesVersions": {
        ">=3.1": {
            "./*": [
                "./dist/types/*"
            ]
        }
    },
    "peerDependenciesMeta": {},
    "bundleDependencies": [],
    "devDependencies": {
        "@clevercanyon/skeleton-dev-deps": "^1.0.54"
    },
    "overrides": {},
    "cpu": [
        "x64",
        "arm64"
    ],
    "os": [
        "darwin",
        "linux"
    ],
    "engines": {
        "node": "^19.2.0",
        "npm": "^8.19.3"
    },
    "scripts": {
        "dev:": "npx vite dev",
        "dev:help": "npx vite dev --help",
        "dev:mode": "npx vite dev --mode=dev",
        "preview:": "npx vite preview",
        "preview:help": "npx vite preview --help",
        "preview:dev": "npx vite preview --mode=dev",
        "build:": "npx vite build",
        "build:help": "npx vite build --help",
        "build:dev": "npx vite build --mode=dev",
        "build:ci": "npx vite build --mode=ci",
        "build:stage": "npx vite build --mode=stage",
        "build:prod": "npx vite build --mode=prod",
        "postbuild:": "npx tsc --emitDeclarationOnly",
        "postbuild:dev": "npx tsc --emitDeclarationOnly",
        "postbuild:ci": "npx tsc --emitDeclarationOnly",
        "postbuild:stage": "npx tsc --emitDeclarationOnly",
        "postbuild:prod": "npx tsc --emitDeclarationOnly",
        "envs:": "./dev/.files/bin/envs.js",
        "envs:help": "./dev/.files/bin/envs.js --help",
        "envs:push": "./dev/.files/bin/envs.js push",
        "envs:pull": "./dev/.files/bin/envs.js pull",
        "envs:keys": "./dev/.files/bin/envs.js keys",
        "envs:encrypt": "./dev/.files/bin/envs.js encrypt",
        "envs:decrypt": "./dev/.files/bin/envs.js decrypt",
        "envs:setup": "./dev/.files/bin/envs.js setup",
        "envs:setup:new": "./dev/.files/bin/envs.js setup --new",
        "install:": "./dev/.files/bin/install.js",
        "install:help": "./dev/.files/bin/install.js --help",
        "install:project": "npm ci --ignore-scripts && ./dev/.files/bin/install.js project",
        "update:": "./dev/.files/bin/update.js",
        "update:help": "./dev/.files/bin/update.js --help",
        "update:dotfiles": "./dev/.files/bin/update.js dotfiles",
        "update:project": "npm run update:dotfiles && ./dev/.files/bin/update.js project",
        "update:project:repos": "npm run update:dotfiles && ./dev/.files/bin/update.js project --repos",
        "update:project:repos:pkgs": "npm run update:dotfiles && ./dev/.files/bin/update.js project --repos --pkgs",
        "update:projects::": "./dev/.files/bin/update.js projects",
        "update:projects::help": "./dev/.files/bin/update.js projects --help",
        "update:projects::dotfiles": "./dev/.files/bin/update.js projects --run update:dotfiles",
        "update:projects::project": "./dev/.files/bin/update.js projects --run update:project",
        "update:projects::project:repos": "./dev/.files/bin/update.js projects --run update:project:repos",
        "update:projects::project:repos:pkgs": "./dev/.files/bin/update.js projects --run update:project:repos:pkgs",
        "wrangler:": "CLOUDFLARE_API_TOKEN=${C10N_CLOUDFLARE_TOKEN} npx wrangler",
        "wrangler:help": "CLOUDFLARE_API_TOKEN=${C10N_CLOUDFLARE_TOKEN} npx wrangler --help"
    },
    "workspaces": [],
    "config": {
        "c10n": {
            "&": {
                "dotfiles": {
                    "lock": []
                },
                "build": {
                    "appType": "cma",
                    "targetEnv": "webw"
                }
            }
        }
    }
}
